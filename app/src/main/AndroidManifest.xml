<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tutoapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeNoActionBar">
        <activity android:name="com.example.tutoapp.PseleccionadoActivity"></activity>
        <activity android:name="com.example.tutoapp.MostrarActivity"
            android:parentActivityName="com.example.tutoapp.HomeActivity"/>
        <activity android:name="com.example.tutoapp.TutorActivity" />
        <activity android:name="com.example.tutoapp.MperfilActivity" />
        <activity android:name="com.example.tutoapp.ProfileActivity" />
        <activity android:name="com.example.tutoapp.AtributesActivity" />
        <activity android:name="com.example.tutoapp.HomeActivity" />
        <activity android:name="com.example.tutoapp.CategoriesActivity" />
        <activity android:name="com.example.tutoapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.tutoapp.Forgotpass_Activity" />
        <activity android:name="com.example.tutoapp.Login_Activity" android:windowSoftInputMode="adjustPan"/>
        <activity android:name="com.example.tutoapp.Register_Activity" android:windowSoftInputMode="adjustPan"/>
    </application>

</manifest>