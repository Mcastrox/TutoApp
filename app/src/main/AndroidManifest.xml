<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.pdm.tutoapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.pdm.tutoapp.VerSolicitudEnviadaActivity"
            android:parentActivityName="com.pdm.tutoapp.fragments.SolicitudEnviada" />
        <activity
            android:name="com.pdm.tutoapp.CentralActivity"
            android:theme="@style/SplashTheme2"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pdm.tutoapp.Change_password"
            android:parentActivityName="com.pdm.tutoapp.fragments.PerfilFragment" />
        <activity
            android:name="com.pdm.tutoapp.MainActivity"
            android:theme="@style/SplashTheme" />
        <activity
            android:name="com.pdm.tutoapp.fragments.SearchFragment"
            tools:ignore="Instantiatable" />
        <activity
            android:name="com.pdm.tutoapp.VerSolicitudActivity"
            android:parentActivityName="com.pdm.tutoapp.TutoriasActivity" />
        <activity android:name="com.pdm.tutoapp.TutoriasActivity" />
        <activity
            android:name="com.pdm.tutoapp.SolicitudActivity"
            android:parentActivityName="com.pdm.tutoapp.fragments.SearchFragment" >
        </activity>
        <activity
            android:name="com.pdm.tutoapp.PseleccionadoActivity"
            android:parentActivityName="com.pdm.tutoapp.fragments.SearchFragment" />
        <activity
            android:name="com.pdm.tutoapp.TutorActivity"
            android:parentActivityName="com.pdm.tutoapp.fragments.PerfilFragment" />
        <activity
            android:name="com.pdm.tutoapp.ProfileActivity"
            android:parentActivityName="com.pdm.tutoapp.fragments.PerfilFragment" />
        <activity
            android:name="com.pdm.tutoapp.AtributesActivity"
            android:parentActivityName="com.pdm.tutoapp.TutoriasActivity" />
        <activity
            android:name="com.pdm.tutoapp.Forgotpass_Activity"
            android:parentActivityName="com.pdm.tutoapp.Change_password" />
        <activity
            android:name="com.pdm.tutoapp.fragments.PerfilFragment"
            tools:ignore="Instantiatable" />
        <activity
            android:name="com.pdm.tutoapp.Login_Activity"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.pdm.tutoapp.Register_Activity"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.pdm.tutoapp.fragments.SolicitudEnviada"
            tools:ignore="Instantiatable" />


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>